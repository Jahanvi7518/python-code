def primeChecker(n):
    if(n>1):
        for i in range(2,int(n/2)+1):
            if(n%i)==0:
                print("not prime number")
                break
            else:
                print("prime number")
    else:
        print("not prime number")            
n=int(input("enter the number:"))
primeChecker(n)
